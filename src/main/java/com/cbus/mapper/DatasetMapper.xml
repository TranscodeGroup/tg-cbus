<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.cbus.mapper.DatasetMapper">

    
    <select id="getData" parameterType="com.cbus.po.DatasetVo" resultType="com.cbus.po.DatasetVo"> 
        SELECT
            t1.*
        FROM
            t_dataset t1
        <where>
            <if test="id != null">
                and t1.id = #{id}
            </if>
            <if test="gid != null and gid !=''">
                and t1.gid = #{gid}
            </if>
            <if test="modid != null and modid !=''">
                and t1.modid = #{modid}
            </if>
        </where>
        <if test="orderby != null and orderby !=''">
            order by ${orderby}
        </if>
    </select>
    
    
</mapper>